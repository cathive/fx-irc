            <Tab text="Untitled Tab 2">
              <content>
                <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
              </content>
            </Tab>
          </tabs>
        </TabPane>
        <ToggleButton contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" text="Nicks" AnchorPane.rightAnchor="4.0" AnchorPane.topAnchor="4.0">
          <graphic>
            <FontAwesomeIconView icon="ICON_TABLE" />
          </graphic>
<tooltip><Tooltip text="Empty Tooltip" />
</tooltip>
        </ToggleButton>
      </children>
    </AnchorPane>
  </center>
  <top>
    <ToolBar maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
      <items>
        <Button mnemonicParsing="false" text="%app.title" />
      </items>
    </ToolBar>
  </top>
